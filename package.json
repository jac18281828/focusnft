{
  "name": "focusnft",
  "version": "0.5.1",
  "description": "OpenZeppelin NFT Contract Complete Example",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "contracts": "contracts",
    "test": "test"
  },
  "scripts": {
    "build": "forge build --sizes",
    "prettier:check": "prettier --check \"(contracts|test)/**.sol\"",
    "prettier:fix": "prettier --write \"(contracts|test)/**.sol\"",
    "hint": "solhint contracts/**/*.sol test/**/*.sol",
    "test": "~mr/.cargo/bin/forge test -vvv",
    "clean": "forge clean && forge cache clean",
    "vanity": "cast create2 --starts-with 0x0c731 --ends-with 01 --deployer 0x4e59b44847b379578588920cA78FbF26c0B4956C --init-code $(cat FocusNFT.bin)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jac18281828/FocusNFT.git"
  },
  "devDependencies": {
    "solhint": "4.0.0"
  },
  "dependencies": {
    "prettier": "3.0.3",
    "prettier-plugin-solidity": "1.1.3"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jac18281828/FocusNFT/issues"
  },
  "homepage": "https://github.com/jac18281828/FocusNFT"
}
